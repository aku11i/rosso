{
  "name": "@aku11i/rosso",
  "version": "0.0.1",
  "description": "Build an RSS feed from various sources",
  "license": "MIT",
  "author": "aku11i",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aku11i/rosso.git"
  },
  "bugs": {
    "url": "https://github.com/aku11i/rosso/issues"
  },
  "homepage": "https://github.com/aku11i/rosso#readme",
  "keywords": [
    "rss",
    "feed",
    "aggregator",
    "cli"
  ],
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "bin": {
    "rosso": "dist/bin.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node dist/bin.mjs",
    "dev": "node --experimental-strip-types src/bin.ts",
    "build": "tsdown src/index.ts src/bin.ts --dts --clean",
    "test": "node --experimental-strip-types --test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "fix": "biome check --write --unsafe .",
    "prepack": "npm run build",
    "prepublishOnly": "npm run typecheck && npm test"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^22.0.0",
    "tsdown": "^0.17.4",
    "typescript": "^5.8.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.86",
    "@rowanmanning/feed-parser": "^2.1.1",
    "ai": "^5.0.113",
    "env-paths": "^3.0.0",
    "feed": "^5.1.0",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  }
}
