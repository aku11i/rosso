{
  "name": "@aku11i/rosso",
  "version": "0.1.0",
  "description": "Build an RSS feed from various sources",
  "private": true,
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "bin": {
    "rosso": "dist/bin.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/bin.mjs",
    "dev": "node --experimental-strip-types src/bin.ts",
    "build": "tsdown src/index.ts src/bin.ts --dts --clean",
    "test": "node --experimental-strip-types --test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "fix": "biome check --write --unsafe ."
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^22.0.0",
    "tsdown": "^0.17.4",
    "typescript": "^5.8.0"
  },
  "dependencies": {
    "@rowanmanning/feed-parser": "^2.1.1",
    "env-paths": "^3.0.0",
    "feed": "^5.1.0",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  }
}
